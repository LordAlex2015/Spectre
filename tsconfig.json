{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2021",
    "module": "CommonJS",
    "declaration": true,
    "strict": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true
  },
}